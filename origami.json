{
	"description": "A framework for building technical documentation and tool UIs",
	"origamiType": "module",
	"origamiVersion": 1,
	"support": "https://github.com/Financial-Times/o-techdocs/issues",
	"supportStatus": "active",
	"browserFeatures": {
		"required": [
			"document.querySelector",
			"Element.prototype.classList",
			"Array.prototype.forEach"
		]
	},
	"ci": {
	  "circle": "https://circleci.com/api/v1/project/Financial-Times/o-techdocs"
	},
	"demosDefaults": {
		"documentClasses": "o-techdocs",
		"sass": "demos/src/scss/main.scss",
		"js": "main.js",
		"dependencies": ["o-fonts@^1.4.0", "o-grid@^4.0.0"]
	},
	"demos": [
		{
		"name": "demo",
		"description": "techdocs demo",
		"template": "demos/src/demo.mustache"
		}
	]
}
